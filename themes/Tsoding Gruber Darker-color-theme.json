{
    "$schema": "vscode://schemas/color-theme",
    "name": "Tsoding Gruber Darker",
    "type": "dark",
    "semanticHighlighting": true,
    // UI colors for VS Code interface parts
    "colors": {
        // Activity Bar: Side bar icons and background
        "activityBar.background": "#282828",
        "activityBar.foreground": "#ffffff",
        "activityBar.inactiveForeground": "#52494e",
        "activityBarBadge.background": "#ffdd33",
        "activityBarBadge.foreground": "#000000",
        // Button colors
        "button.background": "#ffdd33",
        "button.foreground": "#000000",
        // Editor colors: main editor area
        "editor.background": "#181818",
        "editor.foreground": "#f4f4ff",
        "editorCursor.foreground": "#ffdd33",
        "editor.findMatchBackground": "#f4f4ff",
        "editor.findMatchForeground": "#0a0a0a",
        "editor.findMatchHighlightBackground": "#8b8989de",
        "editor.findMatchHighlightForeground": "#000000",
        "editor.hoverHighlightBackground": "#63626231",
        // Editor selections
        "editor.selectionBackground": "#484848",
        "editor.selectionHighlightBackground": "#484848",
        // Editor word highlights
        "editor.wordHighlightBackground": "#00000000",
        "editor.wordHighlightStrongBackground": "#282828",
        // Editor errors and warnings
        "editorError.background": "#00000000",
        "editorError.foreground": "#f43841",
        "editorGutter.background": "#181818",
        "editorGroupHeader.tabsBackground": "#181818",
        "editorInfo.background": "#00000000",
        "editorLineNumber.foreground": "#7a7076",
        "editorLineNumber.activeForeground": "#ffdd33",
        "editorLink.activeForeground": "#96a6c8",
        "editorWarning.background": "#00000000",
        "editorWarning.foreground": "#cc8c3c",
        "editorWidget.background": "#282828",
        // Editor bracket highlighting
        "editorBracketHighlight.foreground1": "#bfc5bf",
        "editorBracketHighlight.foreground2": "#bfc5bf",
        "editorBracketHighlight.foreground3": "#bfc5bf",
        "editorBracketHighlight.foreground4": "#bfc5bf",
        "editorBracketHighlight.foreground5": "#bfc5bf",
        "editorBracketHighlight.foreground6": "#bfc5bf",
        "editorBracketHighlight.unexpectedBracket.foreground": "#f43841",
        // Editor bracket matching
        "editorBracketMatch.background": "#52494e",
        "editorBracketMatch.border": "#00000000",
        // Editor bracket pair guides
        "editorBracketPairGuide.activeBackground1": "#282828",
        "editorBracketPairGuide.activeBackground2": "#282828",
        "editorBracketPairGuide.activeBackground3": "#282828",
        "editorBracketPairGuide.activeBackground4": "#282828",
        "editorBracketPairGuide.activeBackground5": "#282828",
        "editorBracketPairGuide.activeBackground6": "#282828",
        "editorBracketPairGuide.background1": "#52494e",
        "editorBracketPairGuide.background2": "#52494e",
        "editorBracketPairGuide.background3": "#52494e",
        "editorBracketPairGuide.background4": "#52494e",
        "editorBracketPairGuide.background5": "#52494e",
        "editorBracketPairGuide.background6": "#52494e",
        // Side Bar
        "sideBar.background": "#181818",
        // Status Bar
        "statusBar.background": "#282828",
        "statusBar.noFolderBackground": "#282828",
        "statusBar.noFolderForeground": "#ffffff",
        // Text Links
        "textLink.activeForeground": "#96a6c8",
        "textLink.foreground": "#9e95c7",
        // Title Bar
        "titleBar.activeBackground": "#282828",
        "titleBar.border": "#282828"
    },
    // Semantic token colors for language features
    "semanticTokenColors": {
        "newOperator": "#ffdd33",
        "stringLiteral": "#73c936",
        "customLiteral": "#95a99f",
        "numberLiteral": "#95a99f",
        "parameter": {
            "foreground": "#f4f4ff"
        },
        "type": {
            "foreground": "#b6b6b6"
        },
        "namespace": {
            "foreground": "#ece9da"
        },
        "macro": {
            "foreground": "#95a99f"
        }
    },
    // Token colors for syntax highlighting
    "tokenColors": [
        {
            "scope": ["meta.embedded", "source.groovy.embedded", "string meta.image.inline.markdown"],
            "settings": {
                "foreground": "#f4f4ff"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#cc8c3c"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#95A99F"
            }
        },
        {
            "scope": [
                "constant",
                "constant.numeric",
                "variable.other.constant",
                "keyword.constant.bool.zig",
                "keyword.constant.default.zig",
                "keyword.operator.plus.exponent",
                "keyword.operator.minus.exponent"
            ],
            "settings": {
                "foreground": "#95a99f"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#73c936"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "entity.name.tag.css",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#f4f4ff"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.class.mixin.css",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.scss"
            ],
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#f43841"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#9e95c7"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#9e95c7"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9e95c7"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough",
                "foreground": "#9e95c7"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#73c936"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#f43841"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "punctuation.definition.quote.begin.markdown",
            "settings": {
                "foreground": "#cc8c3c"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#73c936"
            }
        },
        {
            "name": "brackets of XML/HTML tags",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#b6b6b6"
            }
        },
        {
            "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "meta.preprocessor.string",
            "settings": {
                "foreground": "#73c936"
            }
        },
        {
            "scope": "meta.preprocessor.numeric",
            "settings": {
                "foreground": "#95a99f"
            }
        },
        {
            "scope": "meta.structure.dictionary.key.python",
            "settings": {
                "foreground": "#f4f4ff"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "storage",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": ["storage.modifier", "keyword.operator.noexcept"],
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": ["string", "meta.embedded.assembly"],
            "settings": {
                "foreground": "#73c936"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#73c936"
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#73c936"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#73c936"
            }
        },
        {
            "name": "String interpolation",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "name": "Reset JavaScript string interpolation expression",
            "scope": ["meta.template.expression"],
            "settings": {
                "foreground": "#f4f4ff"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name",
                "support.type.property-name",
                "variable.css",
                "variable.scss",
                "variable.other.less",
                "source.coffee.embedded"
            ],
            "settings": {
                "foreground": "#f4f4ff"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.cast",
                "keyword.operator.sizeof",
                "keyword.operator.alignof",
                "keyword.operator.typeid",
                "keyword.operator.alignas",
                "keyword.operator.instanceof",
                "keyword.operator.logical.python",
                "keyword.operator.wordlike"
            ],
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#95a99f"
            }
        },
        {
            "scope": ["punctuation.section.embedded.begin.php", "punctuation.section.embedded.end.php"],
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        {
            "scope": "support.function.git-rebase",
            "settings": {
                "foreground": "#f4f4ff"
            }
        },
        {
            "scope": "constant.sha.git-rebase",
            "settings": {
                "foreground": "#95a99f"
            }
        },
        {
            "name": "coloring of the Java import and package identifiers",
            "scope": [
                "storage.modifier.import.java",
                "variable.language.wildcard.java",
                "storage.modifier.array",
                "storage.modifier.package.java"
            ],
            "settings": {
                "foreground": "#e4e4ef"
            }
        },
        {
            "name": "this.self",
            "scope": "variable.language",
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        // Variables
        {
            "scope": [
                "variable",
                "variable.other.readwrite",
                "variable.other.enummember",
                "variable.other.metavariable.name"
            ],
            "settings": {
                "foreground": "#f4f4ff"
            }
        },
        // HTML doctype, Java annotations, Python magic variables/functions/decorators, Types, built-in types, storage types
        {
            "scope": [
                "meta.tag.metadata.doctype.html",
                "storage.type.annotation.java",
                "punctuation.definition.annotation.java",
                "support.variable.magic.python",
                "support.function.magic.python",
                "meta.function.decorator.python",
                "entity.name.type",
                "entity.other.inherited-class",
                "storage.type.java",
                "storage.type.generic.java",
                "storage.type.object.array.java",
                "support.type.julia",
                "entity.name.type.primitive",
                "entity.name.type.numeric",
                "storage.type.primitive",
                "storage.type.built-in.primitive.cpp",
                "storage.type.built-in.primitive.c",
                "storage.type.string.go",
                "storage.type.int.go",
                "storage.type.error.go",
                "storage.type.byte.go",
                "storage.type.numeric.go",
                "storage.type.boolean.go",
                "storage.type.rune.go",
                "storage.type.uintptr.go",
                "keyword.type.zig",
                "keyword.type.integer.zig",
                "keyword.type.c.zig"
            ],
            "settings": {
                "foreground": "#b6b6b6"
            }
        },
        // JSON property names, YAML tag names, C array brackets, C/C++ preprocessor function names
        {
            "scope": [
                "support.type.property-name.json",
                "entity.name.tag.yaml",
                "storage.modifier.array.bracket.square.c",
                "storage.modifier.array.bracket.square.cpp",
                "entity.name.function.preprocessor.c",
                "entity.name.function.preprocessor.cpp"
            ],
            "settings": {
                "foreground": "#e4e4ef"
            }
        },
        // XML tags, HTML tag names, TOML table tags, CSS class and ID names, Result/Option types, Variable specifier
        {
            "scope": [
                "meta.tag.xml",
                "entity.name.tag.html",
                "meta.tag.table.toml",
                "entity.name.type.result",
                "entity.name.type.option",
                "variable.other.metavariable.specifier"
            ],
            "settings": {
                "foreground": "#ffdd33"
            }
        },
        // CSS property values and W3C colors, Rust lifetime, Hexadecimal units
        {
            "scope": [
                "support.constant.property-value.css",
                "support.constant.color.w3c-standard-color-name.css",
                "entity.name.type.lifetime",
                "keyword.other.unit.hexadecimal"
            ],
            "settings": {
                "foreground": "#9e95c7"
            }
        },
        // Token styling - Info
        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796E6"
            }
        },
        // Token styling - Warning
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#CD9731"
            }
        },
        // Token styling - Debug
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#B267E6"
            }
        }
    ]
}
